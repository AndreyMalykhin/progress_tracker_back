{
  "name": "progress_tracker_back",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "bottlejs": "^1.7.0",
    "commander": "^2.15.1",
    "dataloader": "^1.4.0",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "glob": "^7.1.2",
    "graphql": "^0.12.3",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^2.16.0",
    "http-errors": "^1.6.2",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.14.4",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.1",
    "pm2": "^2.10.2",
    "raven": "^2.6.0",
    "sharp": "^0.20.1",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "scripts": {
    "knex-migrate-latest":
      "env-cmd ./.env.local knex --knexfile=build/db-config.js migrate:latest",
    "knex-migrate-latest-prod":
      "env-cmd ./.env.production.local knex --knexfile=build/db-config.js migrate:latest",
    "knex-migrate-rollback":
      "env-cmd ./.env.local knex --knexfile=build/db-config.js migrate:rollback",
    "knex-migrate-make":
      "env-cmd ./.env.local knex --cwd src --knexfile=build/db-config.js migrate:make -x ts",
    "knex-seed-make":
      "env-cmd ./.env.local knex --cwd src --knexfile=build/db-config.js seed:make -x ts",
    "knex-seed-run":
      "env-cmd ./.env.local knex --knexfile=build/db-config.js seed:run",
    "start": "env-cmd ./.env.local nodemon --inspect build/app.js",
    "start-prod": "env-cmd ./.env.production.local node build/app.js",
    "dev": "env-cmd ./.env.local webpack --watch --display-modules",
    "build": "env-cmd ./.env.local webpack --display-modules",
    "build-prod": "env-cmd ./.env.production.local webpack --display-modules",
    "task": "env-cmd ./.env.local node build/task-runner.js",
    "task-prod": "env-cmd ./.env.production.local node build/task-runner.js",
    "docker-compose-prod":
      "env-cmd ./.env.production.local docker-compose -f docker-compose.production.yml",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": ["tslint --fix", "prettier --write", "git add"],
    "*.{js,json}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/debug": "^0.0.30",
    "@types/faker": "^4.1.2",
    "@types/graphql": "^0.13.0",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/knex": "^0.14.9",
    "@types/lodash": "^4.14.107",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^9.6.0",
    "@types/node-fetch": "^1.6.7",
    "@types/raven": "^2.5.1",
    "@types/sharp": "^0.17.8",
    "@types/uuid": "^3.4.3",
    "@types/validator": "^9.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "env-cmd": "^7.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.14.8",
    "prettier": "^1.11.1",
    "raw-loader": "^0.5.1",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^3.0.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.7.2",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dashboard": "^1.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
